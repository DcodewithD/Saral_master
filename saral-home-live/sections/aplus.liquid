<style>
  .aplus {
      padding: 0px 120px;
        display: flex;
      flex-direction: column;
      row-gap: 50px;
  }

  .aplus .section-1, .section-2 {
      display: flex;
    gap:25px;
  }

  .aplus .section-1 .inner, .section-2 .inner {
      flex: 0 0 calc(100%/3 - 25px*2/3);
      max-width: calc(100%/3 - 25px*2/3);
  }

  .aplus .section-3 {
      background: radial-gradient(25.52% 266.75% at 83.25% 50%, {{section.settings.bg-color-s3-1}} 0%, {{section.settings.bg-color-s3-2}} 100%);
