  .aplus .section-3 .inner-3 {
      display: flex;
      flex-direction: column;
      flex: 0 0 65%;
      max-width: 65%;
  }

  .aplus .section-3 .inner-2 {
      display: flex;
  }

  .aplus .section-3 .inner-6 {
      flex: 0 0 30%;
      max-width: 30%;
