<style>
  .kids {
    display: flex;
    align-items: center;
    background: #FAF8F5;
    text-align: center;
    gap: 25px;
        padding: 50px 120px;
    margin: 60px 0px;
}

.kids1 {
    flex: 0 0 calc(100%/3 - 25px*2/3);
    max-width: calc(100%/3 - 25px*2/3);
  position:relative;
}

.kids2 {
    flex: 0 0 calc(100%/3 - 25px*2/3);
    max-width: calc(100%/3 - 25px*2/3);
}

.kids2 p {
    font-weight: 500;
    font-size: 19px;
    line-height: 33px;
    letter-spacing: 0.06em;
    color: #3A3844;
}

.kids2 button {
    width: 219px;
    height: 48px;
    background: #3A3844;
    border-radius: 4px;
    border: none;
    font-weight: 400;
    font-size: 16px;
    line-height: 15px;
    color: #ffffff;
    margin: 20px auto 0px;
    font-family: var(--body_font);
}

.kids3 {
    flex: 0 0 calc(100%/3 - 25px*2/3);
    max-width: calc(100%/3 - 25px*2/3);
  display: flex;
    align-items: center;
    flex-wrap: wrap;
      gap: 25px;
}

.kids4 {
    flex: 0 0 calc(100%/2 - 25px/2);
    max-width: calc(100%/2 - 25px/2);
  position:relative;
}

.kids4 h3 {
    font-size: 18px;
    margin: 0px;
    padding: 10px 0px 0px;
}

.kids4 img {
    border-radius: 8px;
}

.kids1 .card-image-wrap {
    border-top-left-radius: 90px;
    border-top-right-radius: 90px;
}

@media(max-width:768px){
.kids1{
  display:none;
}

.kids {
    flex-direction: column-reverse;
  padding:25px 16px;
  margin: 30px 0px;
}

.kids2 {
    flex: 0 0 100%;
    max-width: 100%;
}

.kids3 {
    flex: 0 0 100%;
    max-width: 100%;
    display: flex;
    align-items: center;
    gap: 25px;
    flex-wrap: nowrap;
    overflow-x: auto;
}

.kids4 {
    flex: 0 0 calc(90%/2 - 25px/2);
    max-width: calc(90%/2 - 25px/2);
}

.kids4 h3 {
    font-size: 15px;
}

.kids2 p {
    font-size: 15px;
    line-height: 26px;
}
  
}
</style>
<div class="kids">
  <div class="kids1">
      {% comment %}<img src="{{ section.settings.section_image  | image_url }}">{% endcomment %}
        {% assign media = section.settings.section_image %}
  <div
        class="card-image-wrap"
        style="position:relative;overflow:hidden;background-color:ghostwhite;padding-bottom:{{ 1 | divided_by : media.aspect_ratio | times : 100 }}%"
      >
        <img loading="lazy"
          srcset="
            {%- if media.width >= 165 -%}{{ media | image_url: width: 165 }} 165w,{%- endif -%}
            {%- if media.width >= 360 -%}{{ media | image_url: width: 360 }} 360w,{%- endif -%}
            {%- if media.width >= 533 -%}{{ media | image_url: width: 533 }} 533w,{%- endif -%}
            {%- if media.width >= 720 -%}{{ media | image_url: width: 720 }} 720w,{%- endif -%}
            {%- if media.width >= 940 -%}{{ media | image_url: width: 940 }} 940w,{%- endif -%}
            {%- if media.width >= 1066 -%}{{ media | image_url: width: 1066 }} 1066w,{%- endif -%}
            {{ media | image_url }} {{ media.width }}w
          "
          src="{{ media | image_url: width: 533 }}"
          alt="{{ media.alt | escape }}"
          loading="lazy"
          sizes=" (min-width: 990px) calc((100vw) / 4), (min-width: 750px) calc((100vw ) / 3), calc((100vw ) / 2)"
          width="{{ card_media.width }}"
          height="{{ card_media.height }}"
          style="position:absolute;top:0px;left:0px;width:100%;height:auto"
        >
</div>
 <a class="url" href="{{ section.settings.redirection_link }}"></a> 
  </div>
