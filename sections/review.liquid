<style>
.slideshow__text.banner__box {
    display: none!important;
}
.slideshow__controls.slideshow__controls--top.slider-buttons.no-js-hidden.slideshow__controls--border-radius-mobile {
    display: none;
}
  h1 {
  margin-top: 32px;
}

.card {
  margin-bottom: 20px;
}

.mySlider {
  position: relative;
  padding-bottom: 40px;
}

.slick-dots {
  display: block;
  position: absolute;
  transform: translateX(-50%);
  left: 50%;
  margin: 0;
  bottom: 10px;
  list-style: none;

  li {
    display: inline-block;
    margin-right: 10px;
    
    button {
      width: 12px;
      height: 12px;
      border: 0;
      border-radius: 100%;
      background-color: rgba(#000, 0.20);
      text-indent: -999999px;
    }

    &.slick-active {
      button {
        background-color: #2b9fdf;
      }
    }
  }
  
}
   .rev ul.slick-dots button {
    display: none!important;
}
  .rev ul.slick-dots li{width:30px;}
   ul.slick-dots li.slick-active{color:red;}
  ul.slick-dots button {
    display: none;
}
  .slick-dots {
    display: inline-flex;
    margin-left:auto;
    margin-right:auto;
  }
  .site-wrapper h2 {
     margin-bottom: 0px; 
}
</style>

